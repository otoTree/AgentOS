{
  "name": "@agentos/desktop",
  "version": "1.0.0",
  "private": true,
  "description": "AgentOS Desktop Client using Electrobun",
  "scripts": {
    "css:build": "bunx tailwindcss -i ./src/mainview/index.css -o ./src/mainview/output.css",
    "dev": "bun run css:build  && electrobun dev",
    "build": "bun run css:build && electrobun build",
    "start": "bun run build && bun run dev"
