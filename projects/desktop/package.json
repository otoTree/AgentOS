    "@radix-ui/react-slot": "^1.2.4",
    "@types/uuid": "^11.0.0",
    "@vscode/ripgrep": "^1.17.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dexie": "^4.2.1",
    "electrobun": "latest",
    "lodash-es": "^4.17.22",
    "lucide-react": "^0.562.0",
